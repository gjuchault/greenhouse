    "chalk": "^4.1.0",
    "classnames": "^2.2.6",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^2.14.0",
    "dotenv": "^8.2.0",
    "express": "^5.0.0-alpha.8",
    "helmet": "^3.23.3",
    "jsonwebtoken": "^8.5.1",
    "nanoevents": "^5.1.8",
    "pg": "^8.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-table": "^7.2.1",
    "regenerator-runtime": "^0.13.5",
    "rfxcom": "^2.1.1",
    "serialport": "^9.0.0",
    "sql-template-strings": "^2.2.2",
    "uuid": "^8.2.0",
    "zod": "^1.7.1"
  },
  "devDependencies": {
    "@svgr/webpack": "^5.4.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/classnames": "^2.2.10",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/helmet": "0.0.47",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/pg": "^7.14.3",
    "@types/react": "^16.9.41",
