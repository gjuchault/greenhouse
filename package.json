{
  "name": "greenhouse",
  "version": "0.1.0",
  "description": "Greenhouse management through a Rapsberry PI and an Arduino",
  "main": "dist/index.js",
  "repository": "git@github.com:gjuchault/greenhouse.git",
  "author": "Gabriel Juchault <gabriel.juchault@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "server:clear": "rm -rf dist/server",
    "server:build": "tsc",
    "server:run": "node dist/server",
    "server:dev": "ts-node-dev src/server/index.ts",
    "app:clear": "rm -rf dist/app",
    "app:build": "NODE_ENV=production webpack",
    "app:dev": "NODE_ENV=development webpack-dev-server",
    "start": "concurrently \"npm:server:build\" \"npm:app:build\" && npm run server:run",
    "dev": "concurrently \"npm:app:dev\" \"npm:server:dev\"",
    "build": "npm run server:build && npm run app:build",
    "clear": "npm run server:clear && npm run app:clear",
    "lint": "prettier --write src"
  },
  "dependencies": {
    "@monaco-editor/react": "^3.3.2",
    "@serialport/parser-delimiter": "^9.0.0",
    "axios": "^0.19.2",
