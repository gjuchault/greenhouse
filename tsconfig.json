{
  "include": ["src/server/**/*"],
  "exclude": ["src/app"],
  "compilerOptions": {
    "target": "ES2019",
    "lib": ["ES2019"],
    "module": "commonjs",
    "outDir": "dist/server",
    "strict": true,
    "strictNullChecks": true,
    "incremental": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "preserve"
  }
}
